{
	"name": "search-plusplus",
	"description": "Search++, a fast indexed full text search VSCode extension",
	"version": "0.0.1",
	"icon": "img/logo.png",
	"publisher": "phil294",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/phil294/search++-vscode-extension"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"TODO"
	],
	"extensionDependencies": [
		"vscode.git"
	],
	"main": "./main.js",
	"contributes": {
		"commands": [
			{
				"command": "search++.search",
				"title": "Search",
				"category": "Search++",
				"icon": "$(search-fuzzy)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "search-plusplus",
					"title": "Search++",
					"icon": "$(search-fuzzy)"
				}
			]
		},
		"views": {
			"search-plusplus": [
				{
					"type": "tree",
					"id": "search++",
					"name": "Search++"
				}
			]
		},
		"configuration": {
			"title": "Search++",
			"properties": {
				"search++.doNotUse": {
					"description": "This setting has no effect.",
					"type": "string",
					"default": "editor",
					"enum": [
						"editor",
						"view"
					],
					"enumDescriptions": [
						"bing",
						"bong"
					]
				}
			}
		}
	},
	"scripts": {},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.5.0",
		"@types/node": "^20.14.5",
		"@types/vscode": "^1.90.0",
		"esbuild": "^0.21.5",
		"eslint": "^9.5.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^17.9.0",
		"eslint-plugin-promise": "^6.2.0",
		"globals": "^15.6.0",
		"npm-check-updates": "^16.14.20",
		"ovsx": "^0.9.1",
		"vsce": "^2.15.0"
	},
	"dependencies": {
		"istextorbinary": "^9.5.0",
		"micromatch": "^4.0.7",
		"node-sqlite3-wasm": "^0.8.16"
	}
}
